 0  LOMEM: 16384
 10 D$ =  CHR$ (4):I% = 0:V% = 0:SU% = 0
 20  HGR : REM  CLEARING HGR, IT'S JUST A BUFFER FOR US
 30  TEXT 
 40  PRINT D$;"OPEN INPUT.TXT"
 50  PRINT D$;"READ INPUT.TXT"
 60  ONERR  GOTO 5000
 70  GET V$: POKE (8192 + I%), VAL (V$):I% = I% + 1: GOTO 70
 100  PRINT I%;" DIGITS"
 110 HI% = I% / 2
 120 I% = I% - 1
 200  FOR C = 0 TO I%
 210  IF C + HI% <  = I% THEN  GOTO 230
 220 T% = C + HI% - I% - 1: GOTO 240
 230 T% = C + HI%
 240  IF  PEEK (8192 + C) =  PEEK (8192 + T%) THEN SU% = SU% +  PEEK (8192 + C)
 245  REM PRINT SU%;" ";C;" ";T%;" COMP: "; PEEK (8192 + C);" " PEEK (8192 + T%)
 250  NEXT C
 260  PRINT SU%
 4990  END 
